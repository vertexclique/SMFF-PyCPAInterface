package org.tubs.epoc.SMFF.PyCPAInterface.ModelExtensions;

import org.python.core.PyObject;
import org.tubs.epoc.SMFF.ModelElements.Platform.AbstractResourceData;
import org.tubs.epoc.SMFF.PyCPAInterface.ModelInterfaces.ResourceType;
import org.tubs.epoc.SMFF.PyCPAInterface.ModelInterfaces.TaskType;

public class PyResource  extends AbstractResourceData{
  // the python object in PyCPA corresponding to this task
  PyObject pyObj = null;
  TaskType resObj = null;

  public PyResource(PyObject pyObj) {
    super();
    this.pyObj = pyObj;
    this.resObj = (TaskType) pyObj.__tojava__(ResourceType.class);
  }
  
  PyObject getPyObject(){
    return pyObj;
  }
}
